            <string>//move
scr_Move_Control()
image_angle=global.gravdir+90

//collide
scr_Move_Collide()
//if room_width*.5-x&gt;global.xspeed x=room_width*.5
//if room_height*.5-y&gt;global.yspeed y=room_height*.5

//gravity
land=0
if place_meeting(x,y-64-yspeed,obj_Map_Mask)
yspeed=-1

if place_meeting(x,y-yspeed+4,obj_Map_Mask)
{yspeed=0; ygrav=0; land=1
move_contact_all(global.gravdir,8)}

//jump
if mouse_check_button(mb_left)
{if land=1
{xspeed+=lengthdir_x(5,global.gravdir+180)
yspeed+=lengthdir_y(5,global.gravdir+180)}}

//restart
if keyboard_check_pressed(ord('R'))
room_restart()

//laser pointer
laser=collision_line(x,y-48,640,mouse_y,obj_Map_Mask,1,0)
if laser=noone
{laserx=640; lasery=mouse_y}
else
{laserx=laser.x; lasery=laser.y}
laser_dist=point_distance(x,y-48,laser.x,laser.y)
</string>
          </argument>
        </arguments>
      </action>
